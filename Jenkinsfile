pipeline {
    agent any

    stages {
        stage('Clone Code') {
            steps {
                echo 'Code cloned from GitHub'
            }
        }

        stage('Build Docker Image') {
            steps {
                echo 'Docker image build step'
            }
        }

        stage('Deploy Application') {
            steps {
                echo 'Application deployed using Docker'
            }
        }
    }
}
